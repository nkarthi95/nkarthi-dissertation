@article{thompson_hierarchically_2019,
	title        = {Hierarchically structured composites and porous materials from soft templates: fabrication and applications},
	shorttitle   = {Hierarchically structured composites and porous materials from soft templates},
	author       = {Thompson, Benjamin R. and Horozov, Tommy S. and Stoyanov, Simeon. D. and Paunov, Vesselin N.},
